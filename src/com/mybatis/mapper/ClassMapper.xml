<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http//mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Class">

<!-- 클래스 생성 -->
	<insert id="create" parameterType="classVO">
	</insert>
	
<!-- 클래스 수정 -->
	<update id="modify" parameterType="classVO">
	</update>
	
<!-- 클래스 삭제 -->
	<update id="del" parameterType="classVO">
	</update>
	
<!-- 지정한 클래스 조회 -->
	<select id="view" parameterType="classVO" resultType="classVO">
	</select>	

<!-- 내가 개설한 클래스 보기 -->
	<select id="viewMine" parameterType="String" resultType="classVO">
	</select>	
	
<!-- 승인 대기 클래스 보기 -->
	<select id="pending" parameterType="String" resultType="classVO">
	</select>
	
<!-- 클래스 승인 -->
	<update id="confirm" parameterType="classVO" resultType="classVO">
	</update>

<!-- 전체 클래스 조회 -->
	<select id="allView" parameterType="classVO" resultType="classVO">
	</select>



</mapper>